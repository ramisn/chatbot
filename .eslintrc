{
  "env": {
    "node": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "airbnb-base"],
  "plugins": ["node", "jest", "babel"],
  "parser": "babel-eslint",
  "rules": {
    "class-methods-use-this": "off",
    "object-curly-newline": ["error", { "consistent": true}],
    "require-jsdoc": 0
  }
}
